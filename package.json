		"prettier": "^2.8.0",
		"typescript": "4.7.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "prettier --write"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start -p 3400",
		"analyze": "cross-env ANALYZE=true next build",
